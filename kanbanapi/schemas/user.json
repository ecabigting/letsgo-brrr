{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 4,
      "maxLength": 50,
      "pattern": "^[a-zA-Z0-9]+$"
    },
    "password": {
      "type": "string",
      "minLength": 8,
      "maxLength": 128
    }
  },
  "required": ["username", "password"],
  "additionalProperties": false
}
